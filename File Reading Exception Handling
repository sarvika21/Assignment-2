    public static void main(String[] args) {
        try {
            [span_0](start_span)FileReader fr = new FileReader("c:/nonexistent/file.txt");[span_0](end_span)
            [span_1](start_span)BufferedReader br = new BufferedReader(fr);[span_1](end_span)
            System.out.println(br.readLine());
